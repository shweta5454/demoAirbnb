<%layout("/layouts/boilerplate")%>
<body>
  <div class="row mt-3">
    <h1 class="col-6 offset-3">SignUp Page</h1>
<div class="col-6 offset-3">


     <form method="POST" action="/signup" novalidate class="needs-validation" >
 
   <div class="col-8 ">
    <div class="mb-3">
      <label class="form-label" for="username">Username</label>
      <input
        name="username"
     id="username"
        class="form-control"
        type="text"
        required
      />
      <div class="valid-feedback">Title looks good</div>
    </div>
    <div class="mb-3">
      <label class="form-label" for="password">Password</label>
      <input
        name="password"
     id="password"
        class="form-control"
        type="password"
        required
      />
      <div class="valid-feedback">password looks good</div>
    </div>
    <div class="mb-3">
      <label class="form-label" for="email">Email</label>
      <input
        name="email"
     id="email"
        class="form-control"
        type="email"
        required
      />
      <div class="valid-feedback">email looks good</div>
    </div>

  

    <button class="btn btn-success ">SignUp</button>
  </form>
  </div>
    </div>
    </body>